Proceso Caso10
	Definir radio Como Entero;
	
	Escribir "Ingrese el radio: ";
	Leer radio;
	
	Definir area, perimetro Como Entero;
	area = 3.14 * (radio ^ 2) ;
	perimetro = radio * 4;
	
	Escribir "RESULTADOS";
	Escribir "----------";
	Escribir "Area..........: ", area ;
	Escribir "Perimetro.....: ", perimetro ;
FinProceso
