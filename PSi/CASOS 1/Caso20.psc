Proceso Caso20
	Definir precio Como Real;
	
	Escribir "Precio del producto....:";
	Leer precio;
	
	Definir incremento Como Real;
	
	incremento = precio + (precio * 42/100);
	
	Escribir "RESULTADOS";
	Escribir "----------";
	Escribir "Precio del producto sin el incremento del 42% :  ", precio;
	Escribir "Precio del producto con el incremento del 42% :  ", incremento;

	
FinProceso
