Proceso Caso8
	Definir lado Como Entero;
	
	Escribir "Ingrese lado del cuadrado: ";
	Leer lado;
	
	Definir area, perimetro Como Entero;
	area = lado ^ 2;
	perimetro = lado * 4;
	
	Escribir "RESULTADOS";
	Escribir "----------";
	Escribir "Area..........: ", area ;
	Escribir "Perimetro.....: ", perimetro ;
	
FinProceso
