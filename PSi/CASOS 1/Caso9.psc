Proceso Caso9
	Definir lado Como Entero;
	Definir altura Como Entero;
	
	Escribir "Ingrese lado 1: ";
	Leer lado;
	
	Escribir "Ingrese lado 2: ";
	Leer altura;
	
	Definir area, perimetro Como Entero;
	area = lado * altura;
	perimetro = 2*lado + 2*altura;
	
	Escribir "RESULTADOS";
	Escribir "----------";
	Escribir "Area..........: ", area ;
	Escribir "Perimetro.....: ", perimetro ;
	
FinProceso
