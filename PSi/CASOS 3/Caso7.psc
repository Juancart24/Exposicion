Proceso Caso7
	Definir nombre Como Texto;
	Definir x Como Entero;
	
	Escribir "Nombre: ";
	Leer nombre;
	
	// Para de froma Ascendente de 1 en 1
	Para x = 1 Hasta 10 Hacer
		Escribir x, ". Bienvenido ", nombre, " al curso.";
	FinPara
	
	// Para de forma Descendente de 2 en 2
	Para x = 20 Hasta 0 Con Paso -2 Hacer
		Escribir "Valor: ", x;
	FinPara
FinProceso
